{
  "version": "0.0.0",
  "private": true,
  "name": "angular-todo-demo",
  "description": "A tutorial todo application for AngularJS using the John Papa Style Guide",
  "repository": "https://github.com/KyleMcNutt/angular-todo-demo",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler-cli": "^5.0.3",
    "@types/lodash": "^4.14.88",
    "@types/systemjs": "^0.20.6",
    "lite-server": "^2.3.0",
    "shelljs": "^0.7.8",
    "systemjs": "^0.20.19",
    "typescript": "^2.6.1"
  },
  "scripts": {
    "prebuild-jit": "yarn run clean-jit",
    "build-jit": "tsc -p tsconfig-jit.json",
    "clean-jit": "node --eval \"require('shelljs').rm('-rf', 'dist-jit')\"",
    "serve-jit": "node --eval \"require('shelljs').cp('index-jit.html', 'index.html')\" && lite-server",
    "prebuild-aot": "yarn run clean-aot",
    "build-aot": "ngc -p tsconfig-aot.json",
    "clean-aot": "node --eval \"require('shelljs').rm('-rf', 'dist-aot')\"",
    "serve-aot": "node --eval \"require('shelljs').cp('index-aot.html', 'index.html')\" && lite-server"
  },
  "dependencies": {
    "@angular/animations": "^5.0.5",
    "@angular/common": "^5.0.3",
    "@angular/compiler": "^5.0.3",
    "@angular/core": "^5.0.3",
    "@angular/forms": "^5.0.3",
    "@angular/platform-browser": "^5.0.3",
    "@angular/platform-browser-dynamic": "^5.0.3",
    "@angular/router": "^5.0.3",
    "bootstrap": "3.3.7",
    "core-js": "^2.5.1",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  }
}
